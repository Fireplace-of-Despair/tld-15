@using Common.Composition
@using TLD15.TagHelpers
@model TLD15.Pages.Contacts.AFeatureContacts.RequestEdit

<tr>
    <td>@Model.Name</td>
    <td>@Model.Url</td>
    <td>
        <button class="outline" hx-get="@Url.Handler("EditItem", new { id = Model.Id })">
            @Globals.Controls.Edit
        </button>
        <button class="outline" hx-delete="@Url.Handler("Item", new { id = Model.Id })" antiforgery="true">
            @Globals.Controls.Delete
        </button>
    </td>
</tr>